{
  "expo": {
    "name": "zotstore",
    "slug": "zotstore",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ZotStore_Logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sanat.zotstore",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "ZotStore requires access to your photo library to allow you to upload product images.",
        "NSPhotoLibraryAddUsageDescription": "ZotStore saves listing photos to your photo library.",
        "NSCameraUsageDescription": "ZotStore uses the camera so you can take pictures of items you're selling."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "package": "com.sanat.zotstore"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "zotstore",
    "extra": {
      "eas": {
        "projectId": "2cc6239c-7f63-4f25-95c2-6bf1b4c5d411"
      }
    }
  }
}
